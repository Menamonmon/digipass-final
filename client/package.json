{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "next lint",
    "relay": "relay-compiler",
    "start": "next start",
    "dev": "next dev",
    "build": "next build",
    "fetch-schema": "get-graphql-schema https://digipass-graphql-api.herokuapp.com/graphql > ./src/graphql/schema.graphql",
