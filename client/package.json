    "@types/uuid": "^8.3.4",
    "autoprefixer": "^10.4.7",
    "babel-plugin-relay": "^14.1.0",
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
    "get-graphql-schema": "^2.1.2",
    "graphql": "^16.5.0",
    "nodemon": "^2.0.19",
    "postcss": "^8.4.14",
    "relay-compiler": "^14.0.0",
    "tailwindcss": "^3.0.24",
    "typescript": "4.6.4"
  }
