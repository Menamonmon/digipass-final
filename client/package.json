    "clsx": "^1.1.1",
    "crypto": "^1.0.1",
    "daisyui": "^2.17.0",
    "gapi-script": "^1.2.0",
    "next": "12.1.6",
    "react": "^18.2.0",
    "react-countdown-circle-timer": "^3.2.1",
    "react-dom": "^18.2.0",
    "react-google-login": "^5.2.2",
    "react-icons": "^4.8.0",
    "react-relay": "^14.0.0",
    "react-toastify": "^9.0.7",
    "react-use-websocket": "^4.2.0",
    "relay-runtime": "^14.0.0",
    "socket.io-client": "^4.6.1",
    "uuid": "^8.3.2"
